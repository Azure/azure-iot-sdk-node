{
  "compilerOptions": {
    "outDir": "./dist",
    "target":"es2018",
    "module": "commonjs",
    "sourceMap": true,
    "declaration": true,
    "noUnusedParameters": true,
    "removeComments": false,
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true
  }
}
