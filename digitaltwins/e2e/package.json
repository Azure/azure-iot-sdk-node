  "description": "Azure IoT Digital Twins End-to-End tests",
  "main": "index.js",
  "private": true,
  "scripts": {
    "lint": "eslint -c ../../.eslintrc.json .",
    "digitaltwins_commands": "mocha --reporter mocha-multi-reporters --reporter-options configFile=../../mocha-reports.json digitaltwin_commands.js",
    "digitaltwins_telemetry": "mocha --reporter mocha-multi-reporters --reporter-options configFile=../../mocha-reports.json digitaltwin_telemetry.js",
    "digitaltwins_properties": "mocha --reporter mocha-multi-reporters --reporter-options configFile=../../mocha-reports.json digitaltwin_properties.js",
    "model_repository_crud": "mocha --reporter mocha-multi-reporters --reporter-options configFile=../../mocha-reports.json model_repository_crud.js",
    "alltest": "npm run  model_repository_crud && npm run digitaltwins_commands && npm run digitaltwins_properties",
    "e2e": "npm -s run lint && npm -s run alltest"
  },
  "repository": {
    "type": "git",
