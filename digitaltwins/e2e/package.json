  "dependencies": {
    "eslint": "^6.0.1",
    "eslint-config-google": "^0.13.0",
    "mocha": "^6.1.4",
    "mocha-junit-reporter": "^1.18.0",
    "mocha-multi-reporters": "^1.1.7",
    "chai": "^4.2.0",
    "debug": "^4.1.1",
    "uuid": "^3.3.2",
    "azure-iot-common": "1.12.0-preview.0",
    "azure-iot-device": "1.12.0-preview.0",
    "azure-iot-device-mqtt": "1.12.0-preview.0",
    "azure-iothub": "1.12.0-preview.0",
    "@azure/event-hubs": "^1.0.8",
    "azure-iot-digitaltwins-model-repository": "1.0.0-preview.0",
    "azure-iot-digitaltwins-service": "1.0.0-preview.0",
    "azure-iot-digitaltwins-device": "1.0.0-preview.0"
  },
