    "ci": "npm -s run lint && npm -s run build && npm -s run alltest-min ",
    "test": "npm -s run lint && npm -s run build && npm -s run unittest",
    "check-cover": "istanbul check-coverage --statements 95 --branches 88 --functions 90 --lines 96"
  },
