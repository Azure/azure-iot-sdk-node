    "machina": "^4.0.1",
    "rhea": "^1.0.2",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "@types/bluebird": "ts2.2",
    "@types/node": "8.9.4",
    "chai": "^4.1.2",
