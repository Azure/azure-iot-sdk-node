  "dependencies": {
    "async": "^2.6.2",
    "azure-iot-amqp-base": "2.2.5",
    "azure-iot-common": "1.9.5",
    "azure-iot-provisioning-device": "1.5.5",
    "bluebird": "^3.5.0",
    "buffer-builder": "^0.2.0",
